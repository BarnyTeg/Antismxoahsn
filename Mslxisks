local found = nil

for _, obj in ipairs(workspace:GetDescendants()) do
    if obj:IsA("BasePart") and obj.Anchored then
        found = obj
        break
    end
end

if found then
    found.CFrame = found.CFrame + Vector3.new(0, 5, 0)
    print("Объект перемещён:", found:GetFullName())
else
    warn("Не найден подходящий объект.")
end
