local player = game:GetService("Players").LocalPlayer
local replicated = game:GetService("ReplicatedStorage")
local pickUpTool = replicated.Remotes.Tool:FindFirstChild("PickUpTool")
local storeItem = replicated.Remotes:FindFirstChild("StoreItem")
local equipObject = replicated.Remotes.Object:FindFirstChild("EquipObject")

-- Вывод текущего инвентаря
print("Твой инвентарь сейчас:")
for _, v in pairs(player.Backpack:GetChildren()) do
    print(v.Name)
end

-- Дюп Shovel в инвентарь
local shovelName = "Shovel" -- Точное имя предмета
if pickUpTool or storeItem or equipObject then
    print("Начинаю дюп Shovel в инвентарь!")
    
    -- Тест PickUpTool
    if pickUpTool then
        for i = 1, 50 do
            pickUpTool:FireServer(shovelName) -- Спамим имя лопаты
            wait(0.01)
        end
        print("Спамил PickUpTool, чекни инвентарь!")
    end
    
    -- Тест StoreItem
    if storeItem then
        for i = 1, 50 do
            storeItem:FireServer(shovelName)
            wait(0.01)
        end
        print("Спамил StoreItem, чекни инвентарь!")
    end
    
    -- Тест EquipObject
    if equipObject then
        for i = 1, 50 do
            equipObject:FireServer(shovelName)
            wait(0.01)
        end
        print("Спамил EquipObject, чекни инвентарь!")
    end
else
    print("Ни один RemoteEvent не нашёл, пиздец!")
end

-- Проверка инвентаря после спама
wait(1)
print("Твой инвентарь после дюпа:")
for _, v in pairs(player.Backpack:GetChildren()) do
    print(v.Name)
end
