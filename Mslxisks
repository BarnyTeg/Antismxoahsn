-- Получение всех объектов в workspace
local objects = workspace:GetDescendants()

-- Создание строки с информацией о всех объектах
local infoString = ""
for _, object in pairs(objects) do
    local info = "Название: " .. object.Name .. "\nПуть: " .. object:GetFullName() .. "\nAPI: " .. object.ClassName .. "\n"

    -- Добавление атрибутов объекта
    if #object:GetAttributes() > 0 then
        info = info .. "Атрибуты:\n"
        for attributeName, attributeValue in pairs(object:GetAttributes()) do
            info = info .. "  " .. attributeName .. ": " .. tostring(attributeValue) .. "\n"
        end
    else
        info = info .. "Атрибуты: Нет\n"
    end

    -- Добавление свойств для BasePart
    if object:IsA("BasePart") then
        info = info .. "Позиция: " .. tostring(object.Position) .. "\n"
        info = info .. "Размер: " .. tostring(object.Size) .. "\n"
        info = info .. "Цвет: " .. tostring(object.Color) .. "\n"
        info = info .. "Прозрачность: " .. tostring(object.Transparency) .. "\n"
        info = info .. "Материал: " .. tostring(object.Material) .. "\n"
    end

    -- Добавление информации для моделей
    if object:IsA("Model") then
        info = info .. "Количество частей: " .. #object:GetChildren() .. "\n"
        info = info .. "Первичная часть: " .. (object.PrimaryPart and object.PrimaryPart.Name or "Нет") .. "\n"
    end

    -- Добавление информации о родителе
    info = info .. "Родитель: " .. object.Parent.Name .. "\n"

    infoString = infoString .. info .. "\n"
end

-- Проверка, что строка не пустая перед копированием
if infoString ~= "" then
    setclipboard(infoString)
    print("Информация скопирована в буфер обмена")
else
    print("Нет информации для копирования.")
end
