local player = game.Players.LocalPlayer local userInputService = game:GetService("UserInputService") local runService = game:GetService("RunService") local setclipboard = setclipboard or toclipboard or function() end -- Функция копирования в буфер обмена

-- Создаём GUI local screenGui = Instance.new("ScreenGui") screenGui.Parent = game:GetService("CoreGui") -- Делаем меню поверх игры

local logFrame = Instance.new("Frame", screenGui) local logTextBox = Instance.new("TextBox", logFrame) local closeButton = Instance.new("TextButton", logFrame)

-- Настройки окна logFrame.Size = UDim2.new(0.4, 0, 0.5, 0) logFrame.Position = UDim2.new(0.3, 0, 0.2, 0) logFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30) logFrame.Active = true logFrame.Draggable = true

-- Поле для текста логов logTextBox.Size = UDim2.new(1, 0, 0.9, 0) logTextBox.Position = UDim2.new(0, 0, 0, 0) logTextBox.BackgroundTransparency = 1 logTextBox.TextColor3 = Color3.new(1, 1, 1) logTextBox.TextXAlignment = Enum.TextXAlignment.Left logTextBox.TextYAlignment = Enum.TextYAlignment.Top logTextBox.ClearTextOnFocus = false logTextBox.MultiLine = true logTextBox.Text = "Логи взаимодействия с предметами:\n"

-- Кнопка закрытия closeButton.Size = UDim2.new(1, 0, 0.1, 0) closeButton.Position = UDim2.new(0, 0, 0.9, 0) closeButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50) closeButton.TextColor3 = Color3.new(1, 1, 1) closeButton.Text = "Закрыть"

closeButton.MouseButton1Click:Connect(function() screenGui:Destroy() end)

-- Функция добавления логов и копирования в буфер local function addLog(text) logTextBox.Text = logTextBox.Text .. text .. "\n" setclipboard(logTextBox.Text) -- Копирование логов в буфер обмена end

-- Логирование предметов при взаимодействии local function trackItems() local target = player:GetMouse().Target if target then local fullPath = target:GetFullName() addLog("Взаимодействие с предметом: " .. target.Name) addLog("Полный путь: " .. fullPath)

-- Вывод API предмета
    for _, prop in pairs(target:GetChildren()) do
        addLog("Свойство: " .. prop.Name .. " | Класс: " .. prop.ClassName)
    end
end

end

runService.RenderStepped:Connect(trackItems)

