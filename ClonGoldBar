local goldBar = game.Workspace.RuntimeItems.GoldBar.Prop_GoldBar
local function createUniqueObject()
    local newObject = Instance.new("MeshPart")
    newObject.Size = Vector3.new(math.random(1, 5), math.random(1, 5), math.random(1, 5))
    newObject.BrickColor = BrickColor.new(math.random(), math.random(), math.random())
    newObject.Material = Enum.Material.Metal
    newObject.Transparency = math.random()
    newObject.Position = goldBar.Position + Vector3.new(math.random(-10, 10), math.random(-10, 10), math.random(-10, 10))
    newObject.Parent = goldBar.Parent
    return newObject
end

while true do
    local newObject = createUniqueObject()
    wait(0.1)
end
