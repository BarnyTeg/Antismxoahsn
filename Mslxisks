local function createMenu()
    local menu = Instance.new("ScreenGui")
    menu.Name = "ObjectMenu"
    menu.Parent = game.StarterGui

    local objectList = Instance.new("Frame")
    objectList.Name = "ObjectList"
    objectList.Size = UDim2.new(0, 200, 0, 300)
    objectList.Position = UDim2.new(0, 10, 0, 10)
    objectList.Parent = menu

    local objectScroll = Instance.new("ScrollingFrame")
    objectScroll.Name = "ObjectScroll"
    objectScroll.Size = UDim2.new(0, 200, 0, 280)
    objectScroll.Position = UDim2.new(0, 0, 0, 20)
    objectScroll.Parent = objectList

    local objects = workspace:GetDescendants()
    for _, object in pairs(objects) do
        if object:IsA("BasePart") then
            local objectButton = Instance.new("TextButton")
            objectButton.Name = object.Name
            objectButton.Text = object.Name
            objectButton.Size = UDim2.new(0, 180, 0, 20)
            objectButton.Position = UDim2.new(0, 10, 0, 10)
            objectButton.Parent = objectScroll

            local copyButton = Instance.new("TextButton")
            copyButton.Name = "CopyButton"
            copyButton.Text = "Копировать"
            copyButton.Size = UDim2.new(0, 50, 0, 20)
            copyButton.Position = UDim2.new(0, 200, 0, 10)
            copyButton.Parent = objectScroll

            copyButton.MouseButton1Click:Connect(function()
                local newObject = object:Clone()
                newObject.Parent = workspace
                newObject:SetPrimaryPartCFrame(object:GetPrimaryPartCFrame() + Vector3.new(0, 0, 5))
            end)
        end
    end
end

createMenu()
