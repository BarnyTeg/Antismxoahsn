local RS = game:GetService("ReplicatedStorage")
local cloned = RS:WaitForChild("ClonedItems")

local bandage = cloned:FindFirstChild("Bandage")
if bandage then
	print("Бандаж найден в ClonedItems")

	local copy = bandage:Clone()
	copy.Parent = workspace
	print("Бандаж создан в Workspace")
else
	warn("Бандаж не найден в ClonedItems")
end
