local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local radius = 10

-- Функция для сбора информации об объектах в радиусе
local function gatherInfo()
    local objectsInfo = {}
    local position = character.HumanoidRootPart.Position

    -- Проходим по всем объектам в игре
    for _, object in pairs(workspace:GetDescendants()) do
        if (object:IsA("BasePart") or object:IsA("Model")) and object:IsDescendantOf(workspace) then
            local distance = (object.Position - position).magnitude
            if distance <= radius then
                local info = {
                    name = object.Name,
                    type = object.ClassName,
                    parent = object.Parent.Name,
                    position = object.Position,
                    properties = {}
                }
                -- Собираем свойства объекта
                for _, property in pairs(object:GetAttributes()) do
                    table.insert(info.properties, property.Name .. ": " .. tostring(property.Value))
                end
                table.insert(objectsInfo, info)
            end
        end
    end

    -- Выводим информацию об объектах
    for _, info in pairs(objectsInfo) do
        print("Объект: " .. info.name)
        print("Тип: " .. info.type)
        print("Родитель: " .. info.parent)
        print("Позиция: " .. tostring(info.position))
        print("Свойства:")
        for _, property in pairs(info.properties) do
            print(property)
        end
        print()
    end
end

-- Запускаем сбор информации
gatherInfo()
