local ReplicatedStorage = game:GetService("ReplicatedStorage")
local remote = ReplicatedStorage:FindFirstChild("RequestStartDrag")

if remote and remote:IsA("RemoteEvent") then
    remote:FireServer(true, nil, "Bandage")
    print("Вызов RequestStartDrag отправлен")
else
    warn("Remote 'RequestStartDrag' не найден")
end
