local function dumpObjects()
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("ModuleScript") or obj:IsA("RemoteEvent") then
            print(string.format("[%s] %s", obj.ClassName, obj:GetFullName()))
        end
    end
    setclipboard("Данные скопированы в консоль (F9 → All).")
end

dumpObjects()
