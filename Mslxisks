local Players = game:GetService("Players")
local player = Players.LocalPlayer
local bandage = workspace:WaitForChild("RuntimeItems"):WaitForChild("Bandage")
local debugLogs = {} -- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–µ–º –º–∞—Å—Å–∏–≤ –¥–ª—è –ª–æ–≥–æ–≤

-- 1. –ü–æ–∏—Å–∫ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö –∫–Ω–æ–ø–æ–∫
local function findExistingButtons()
    local logText = "üîç –ù–∞–π–¥–µ–Ω–Ω—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è:\n"
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ ProximityPrompt
    if bandage:FindFirstChildWhichIsA("ProximityPrompt") then
        local prompt = bandage:FindFirstChildWhichIsA("ProximityPrompt")
        logText = logText .. "\nüéØ ProximityPrompt: "..prompt.Name
        logText = logText .. "\n   –¢–µ–∫—Å—Ç: "..prompt.ActionText
    else
        logText = logText .. "\n‚ö†Ô∏è ProximityPrompt –Ω–µ –Ω–∞–π–¥–µ–Ω"
    end

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ GUI —ç–ª–µ–º–µ–Ω—Ç–æ–≤
    pcall(function()
        local gui = player.PlayerGui:FindFirstChild("BandageUI")
        if gui then
            logText = logText .. "\n\nüì± GUI —ç–ª–µ–º–µ–Ω—Ç—ã:"
            for _, element in ipairs(gui:GetDescendants()) do
                if element:IsA("TextButton") then
                    logText = logText .. "\n- "..element.Name
                end
            end
        else
            logText = logText .. "\n‚ö†Ô∏è GUI –Ω–µ –Ω–∞–π–¥–µ–Ω"
        end
    end)

    table.insert(debugLogs, logText)
    pcall(function() setclipboard(logText) end)
    print("‚úÖ –î–∞–Ω–Ω—ã–µ —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω—ã!")
end

-- 2. –¢–µ—Å—Ç–æ–≤—ã–µ –∫–Ω–æ–ø–∫–∏
local function createTestButtons()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "BandageDebugUI"
    screenGui.Parent = player.PlayerGui

    -- –ö–Ω–æ–ø–∫–∞ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è
    local useBtn = Instance.new("TextButton")
    useBtn.Name = "MobileUseButton"
    useBtn.Size = UDim2.new(0.4, 0, 0.08, 0)
    useBtn.Position = UDim2.new(0.3, 0, 0.8, 0)
    useBtn.Text = "–ò–°–ü–û–õ–¨–ó–û–í–ê–¢–¨"
    useBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
    useBtn.Parent = screenGui

    -- –ö–Ω–æ–ø–∫–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏
    local infoBtn = Instance.new("TextButton")
    infoBtn.Name = "MobileInfoButton"
    infoBtn.Size = UDim2.new(0.4, 0, 0.08, 0)
    infoBtn.Position = UDim2.new(0.3, 0, 0.9, 0)
    infoBtn.Text = "–ò–ù–§–û"
    infoBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
    infoBtn.Parent = screenGui

    -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏
    useBtn.MouseButton1Down:Connect(function()
        local log = "üïπÔ∏è –ù–∞–∂–∞—Ç–∞ –∫–Ω–æ–ø–∫–∞ –ò–°–ü–û–õ–¨–ó–û–í–ê–¢–¨: " .. os.date("%X")
        print(log)
        table.insert(debugLogs, log)
    end)

    infoBtn.MouseButton1Down:Connect(function()
        local log = "‚ÑπÔ∏è –ù–∞–∂–∞—Ç–∞ –∫–Ω–æ–ø–∫–∞ –ò–ù–§–û: " .. os.date("%X")
        print(log)
        table.insert(debugLogs, log)
    end)
end

-- 3. –ê–≤—Ç–æ–ª–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –¥–µ–π—Å—Ç–≤–∏–π
local function setupAutoLogger()
    -- –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –∫–∞—Å–∞–Ω–∏–π
    game:GetService("UserInputService").TouchStarted:Connect(function(touch, processed)
        if not processed then
            local log = "üñê –ö–∞—Å–∞–Ω–∏–µ: " .. tostring(touch.Position.X) .. ", " .. tostring(touch.Position.Y)
            print(log)
            table.insert(debugLogs, log)
        end
    end)

    -- –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ ProximityPrompt
    if bandage:FindFirstChildWhichIsA("ProximityPrompt") then
        bandage.ProximityPrompt.Triggered:Connect(function()
            local log = "‚ö° ProximityPrompt –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω: " .. os.date("%X")
            print(log)
            table.insert(debugLogs, log)
        end)
    end
end

-- –ó–∞–ø—É—Å–∫ —Å–∏—Å—Ç–µ–º—ã
findExistingButtons()
createTestButtons()
setupAutoLogger()

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è –ª–æ–≥–æ–≤
local copyButton = Instance.new("TextButton")
copyButton.Size = UDim2.new(0.4, 0, 0.08, 0)
copyButton.Position = UDim2.new(0.3, 0, 0.7, 0)
copyButton.Text = "–ö–û–ü–ò–†–û–í–ê–¢–¨ –õ–û–ì–ò"
copyButton.BackgroundColor3 = Color3.fromRGB(255, 100, 0)
copyButton.Parent = player.PlayerGui.BandageDebugUI

copyButton.MouseButton1Down:Connect(function()
    if #debugLogs > 0 then
        pcall(function()
            setclipboard(table.concat(debugLogs, "\n"))
            copyButton.Text = "‚úÖ –°–ö–û–ü–ò–†–û–í–ê–ù–û!"
            wait(1)
            copyButton.Text = "–ö–û–ü–ò–†–û–í–ê–¢–¨ –õ–û–ì–ò"
        end)
    else
        copyButton.Text = "‚ö†Ô∏è –õ–û–ì–ò –ü–£–°–¢–´!"
        wait(1)
        copyButton.Text = "–ö–û–ü–ò–†–û–í–ê–¢–¨ –õ–û–ì–ò"
    end
end)
