local runtimeItems = workspace:WaitForChild("RuntimeItems")

runtimeItems.ChildAdded:Connect(function(child)
    print("[!] Новый объект в RuntimeItems:", child.Name)
    task.wait(0.2)
    for _, attr in ipairs(child:GetAttributes()) do
        print("   Атрибут:", attr, "=", child:GetAttribute(attr))
    end
    for _, sub in ipairs(child:GetChildren()) do
        print("   Содержит объект:", sub.Name, sub.ClassName)
    end
end)
