local Players = game:GetService("Players")
local player = Players.LocalPlayer
local RunService = game:GetService("RunService")

-- 1. REAL GOD MODE (из популярного чит-меню)
local function realGodMode()
    local function protect()
        if not player.Character then return end
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return end

        -- Основная защита
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
        
        -- Анти-баг система
        if humanoid.Health <= 0 then
            player:LoadCharacter()
        end
        
        -- Обход кастомного урона
        for _, v in pairs(getconnections(humanoid.HealthChanged)) do
            v:Disable()
        end
    end

    -- Постоянная защита
    while task.wait(0.1) do
        pcall(protect)
    end
end

-- 2. NPC TERMINATOR (из скриптов 2024)
local function killNPC()
    while task.wait(0.2) do
        pcall(function()
            for _, npc in pairs(workspace:GetDescendants()) do
                if npc:IsA("Model") and npc:FindFirstChildOfClass("Humanoid") then
                    if not Players:GetPlayerFromCharacter(npc) then
                        -- Жёсткое удаление
                        for _, part in pairs(npc:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part:Destroy()
                            end
                        end
                        npc:Destroy()
                    end
                end
            end
        end)
    end
end

-- 3. ANTI-ANTICHEAT (обход защиты)
local function bypassAC()
    if not hookfunction then return end
    
    -- Подмена ключевых функций
    local originalNamecall
    originalNamecall = hookfunction(getrawmetatable(game).__namecall, function(self, ...)
        local method = getnamecallmethod()
        if method == "Kick" or method == "Ban" then
            return nil
        end
        return originalNamecall(self, ...)
    end)
end

-- Запуск
coroutine.wrap(realGodMode)()
coroutine.wrap(killNPC)()
pcall(bypassAC)

print("㊙️ CHEAT ACTIVATED | God Mode + NPC Killer + AC Bypass")
