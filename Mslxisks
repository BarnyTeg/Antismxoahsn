local ReplicatedStorage = game:GetService("ReplicatedStorage")
local log = ""

for _, remote in ipairs(ReplicatedStorage:GetDescendants()) do
    if remote:IsA("RemoteEvent") then
        remote.OnClientEvent:Connect(function(...)
            local args = {...}
            local argStrings = {}
            for _, v in ipairs(args) do
                table.insert(argStrings, typeof(v) == "string" and v or tostring(v))
            end
            log = log .. "\n[REMOTE] "..remote.Name.." | Аргументы: "..table.concat(argStrings, ", ")
            print(log)
            pcall(function() setclipboard(log) end)
        end)
    end
end

print("Логирование Remotes запущено.")
