local function getObjects()
    local objects = {}
    for _, object in pairs(workspace:GetDescendants()) do
        if object.Name then
            table.insert(objects, object.Name .. " (" .. tostring(object) .. ")")
        end
    end
    return objects
end

local function copyIds()
    local objects = getObjects()
    local text = table.concat(objects, "\n")
    setclipboard(text)
    print("Айди и названия объектов скопированы в буфер обмена!")
end

copyIds()
