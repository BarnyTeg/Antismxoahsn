local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

-- Папка, где хранится клон
local clonedFolder = ReplicatedStorage:WaitForChild("ClonedItems")
local clonedBandage = clonedFolder:FindFirstChild("Bandage")

if clonedBandage then
    local newBandage = clonedBandage:Clone()
    newBandage.Parent = Workspace.RuntimeItems
    print("[+] Бандаж успешно вставлен в RuntimeItems")
else
    warn("[-] Клонированный бандаж не найден в ClonedItems")
end
