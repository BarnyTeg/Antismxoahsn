-- Dead Rails End TP Script for Delta Executor
local player = game.Players.LocalPlayer
local hrp = player.Character.HumanoidRootPart
local teleportPos = CFrame.new(15000, 100, 0) -- Примерные координаты финала

-- Функция телепорта
local function teleportToEnd()
    if hrp then
        hrp.CFrame = teleportPos
        print("Телепортнулся к финалу! GLHF!")
    else
        print("Персонаж не найден, попробуй снова.")
    end
end

-- Выполняем тп
teleportToEnd()

-- Опционально: NoClip, чтоб не застрять
local RunService = game:GetService("RunService")
local noclip = true

RunService.Stepped:Connect(function()
    if noclip then
        for _, v in pairs(player.Character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end
end)
