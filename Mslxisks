local player = game:GetService("Players").LocalPlayer
local replicated = game:GetService("ReplicatedStorage")
local dropRemote = replicated.Remotes:FindFirstChild("DropItem")

if dropRemote then
    for i = 1, 50 do
        dropRemote:FireServer("GoldBar") -- Спамим сброс GoldBar
        wait(0.05) -- Тайминг, чтоб сервер лагнул
    end
    print("Заспамил DropItem, чекни землю — есть ли куча GoldBar?")
else
    print("DropItem не нашёл, пиздец!")
end
