local player = game.Players.LocalPlayer
local buttons = ""

for _, gui in ipairs(player.PlayerGui:GetDescendants()) do
    if gui:IsA("TextButton") and (gui.Name:find("Bandage") or gui.Name:find("Take") or gui.Name:find("Use")) then
        buttons = buttons .. "\n"..gui:GetFullName().." | Текст: "..gui.Text
    end
end

print("Найденные кнопки:"..buttons)
setclipboard(buttons)
