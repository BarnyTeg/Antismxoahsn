local function копироватьОбъекты()
    local игрок = game.Players.LocalPlayer
    local персонаж = игрок.Character
    local позиция = персонаж.HumanoidRootPart.Position

    for _, объект in pairs(workspace:GetDescendants()) do
        if объект:IsA("BasePart") and (объект.Position - позиция).Magnitude <= 10 then
            local новыйОбъект = объект:Clone()
            новыйОбъект.Parent = workspace
            новыйОбъект:SetPrimaryPartCFrame(объект:GetPrimaryPartCFrame() + Vector3.new(0, 0, 5))
        end
    end
end

while true do
    копироватьОбъекты()
    wait(1)
end
