local ScreenGui = game:GetObjects("rbxassetid://–í–ê–®_–≠–ö–°–ü–õ–û–ò–¢_–ì–£–ò_ID")[1]
ScreenGui.Parent = game:GetService("CoreGui")

local function getObjectInfo(obj)
    return string.format("Class: %s | Name: %s | Path: %s\nMemory: %sMB", 
        obj.ClassName, 
        obj.Name, 
        obj:GetFullName(),
        math.round(game:GetService("Stats"):GetTotalMemoryUsageMb()/1000, 2)
    )
end

workspace.DescendantAdded:Connect(function(obj)
    local Button = TemplateButton:Clone()
    Button.Text = "üö© "..obj.Name..(obj:IsA("BasePart") and " [PART]" or "")
    Button.MouseButton1Click:Connect(function()
        syn.write_clipboard(tostring(obj:GetDebugId())) -- –î–ª—è —ç–∫—Å–ø–ª–æ–π—Ç–æ–≤
    end)
    Button.Parent = ScrollingFrame
end)
