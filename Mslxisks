local players = game:GetService("Players")
local player = players.LocalPlayer
local character = player.Character
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local function getObjectsInRadius(radius)
    local objects = {}
    for _, object in pairs(workspace:GetDescendants()) do
        if object:IsA("BasePart") then
            local distance = (object.Position - humanoidRootPart.Position).Magnitude
            if distance <= radius then
                table.insert(objects, {id = object.Id, name = object.Name})
            end
        end
    end
    return objects
end

local function printObjects()
    local objects = getObjectsInRadius(10)
    for _, object in pairs(objects) do
        print("Айди: " .. object.id .. ", Название: " .. object.name)
    end
end

while true do
    printObjects()
    wait(1)
end
