local ReplicatedStorage = game:GetService("ReplicatedStorage")
local log = ""

for _, remote in ipairs(ReplicatedStorage:GetDescendants()) do
    if remote:IsA("RemoteEvent") then
        remote.OnClientEvent:Connect(function(...)
            local args = {...}
            log = log .. "\n[REMOTE] "..remote.Name.." | Аргументы: "..table.concat(args, ", ")
            print(log)
            setclipboard(log)
        end)
    end
end

print("Логирование Remotes запущено. Нажмите кнопки бандажа и пришлите логи из консоли (F9)")
