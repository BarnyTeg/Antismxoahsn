local radius = 15 -- Ñ€Ð°Ð´Ð¸ÑƒÑ ÑÐºÐ°Ð½Ð¸Ñ€Ð¾Ð²Ð°Ð½Ð¸Ñ Ð² Ð¼ÐµÑ‚Ñ€Ð°Ñ…
local includeChildren = true -- Ð²ÐºÐ»ÑŽÑ‡Ð°Ñ‚ÑŒ Ð»Ð¸ Ð¸Ð½Ñ„Ð¾Ñ€Ð¼Ð°Ñ†Ð¸ÑŽ Ð¾ Ð´Ð¾Ñ‡ÐµÑ€Ð½Ð¸Ñ… Ð¾Ð±ÑŠÐµÐºÑ‚Ð°Ñ…

local function getPlayerPosition()
    local player = game:GetService("Players").LocalPlayer
    return player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") 
        and player.Character.HumanoidRootPart.Position or Vector3.new(0, 0, 0)
end

local function safeToString(value)
    local success, result = pcall(function()
        if typeof(value) == "Instance" then
            return value:GetFullName()
        elseif typeof(value) == "Vector3" then
            return string.format("(%.2f, %.2f, %.2f)", value.X, value.Y, value.Z)
        elseif typeof(value) == "Color3" then
            return string.format("(%.2f, %.2f, %.2f)", value.R, value.G, value.B)
        else
            return tostring(value)
        end
    end)
    return success and result or "N/A"
end

local function getExtendedInfo(obj)
    local info = {}
    
    -- Ð‘Ð°Ð·Ð¾Ð²Ñ‹Ðµ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð°
    table.insert(info, "ðŸ”¹ Class: "..safeToString(obj.ClassName))
    table.insert(info, "ðŸ”¹ Name: "..safeToString(obj.Name))
    table.insert(info, "ðŸ”¹ FullPath: "..safeToString(obj:GetFullName()))
    table.insert(info, "ðŸ”¹ RobloxID: "..safeToString(obj:GetDebugId()))
    
    -- Ð¤Ð¸Ð·Ð¸Ñ‡ÐµÑÐºÐ¸Ðµ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð°
    if obj:IsA("BasePart") then
        table.insert(info, "ðŸ“ Position: "..safeToString(obj.Position))
        table.insert(info, "ðŸ“ Size: "..safeToString(obj.Size))
        table.insert(info, "ðŸ§± Material: "..safeToString(obj.Material))
        table.insert(info, "ðŸ”˜ Anchored: "..safeToString(obj.Anchored))
        table.insert(info, "ðŸ’¥ CanCollide: "..safeToString(obj.CanCollide))
        table.insert(info, "ðŸ‘ï¸ Transparency: "..safeToString(obj.Transparency))
    end
    
    -- Ð”Ð»Ñ Ð¼Ð¾Ð´ÐµÐ»ÐµÐ¹
    if obj:IsA("Model") then
        table.insert(info, "ðŸ§© PrimaryPart: "..safeToString(obj.PrimaryPart))
        table.insert(info, "ðŸ§’ Children: "..safeToString(#obj:GetChildren()))
    end
    
    -- Ð’Ð¸Ð·ÑƒÐ°Ð»ÑŒÐ½Ñ‹Ðµ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð°
    if obj:IsA("GuiObject") then
        table.insert(info, "ðŸŽ¨ BackgroundColor: "..safeToString(obj.BackgroundColor3))
    elseif obj:IsA("BasePart") then
        table.insert(info, "ðŸ–¼ï¸ TextureCount: "..safeToString(#obj:GetChildrenOfClass("Texture")))
    end
    
    -- Ð¡Ð¿ÐµÑ†Ð¸Ñ„Ð¸Ñ‡Ð½Ñ‹Ðµ ÑÐ²Ð¾Ð¹ÑÑ‚Ð²Ð°
    if obj:IsA("Light") then
        table.insert(info, "ðŸ’¡ Brightness: "..safeToString(obj.Brightness))
        table.insert(info, "ðŸŒˆ Color: "..safeToString(obj.Color))
    end
    
    -- Ð Ð°ÑÑÑ‚Ð¾ÑÐ½Ð¸Ðµ Ð´Ð¾ Ð¸Ð³Ñ€Ð¾ÐºÐ°
    local rootPart = obj:IsA("BasePart") and obj or obj:FindFirstChildOfClass("BasePart") or obj.PrimaryPart
    if rootPart then
        local dist = (rootPart.Position - getPlayerPosition()).Magnitude
        table.insert(info, "ðŸ“ Distance: "..string.format("%.1f", dist).."m")
    end
    
    -- Ð”Ð¾Ð¿Ð¾Ð»Ð½Ð¸Ñ‚ÐµÐ»ÑŒÐ½Ñ‹Ðµ Ð¼ÐµÑ‚Ð°Ð´Ð°Ð½Ð½Ñ‹Ðµ
    table.insert(info, "ðŸ•’ CreatedTime: "..safeToString(obj:GetAttribute("CreatedTime")))
    
    -- Ð¢ÐµÐ³Ð¸
    local tags = {}
    for _, tag in ipairs(game:GetService("CollectionService"):GetTags(obj)) do
        table.insert(tags, tag)
    end
    table.insert(info, "ðŸ·ï¸ Tags: "..(#tags > 0 and table.concat(tags, ", ") or "None"))
    
    -- Ð”Ð¾Ñ‡ÐµÑ€Ð½Ð¸Ðµ Ð¾Ð±ÑŠÐµÐºÑ‚Ñ‹
    if includeChildren and #obj:GetChildren() > 0 then
        table.insert(info, "\nðŸ§’ Children ("..#obj:GetChildren().."):")
        for i, child in ipairs(obj:GetChildren()) do
            if i <= 5 then
                table.insert(info, string.format("  %d. %s (%s)", i, safeToString(child.Name), safeToString(child.ClassName)))
            end
        end
        if #obj:GetChildren() > 5 then
            table.insert(info, string.format("  ... and %d more", #obj:GetChildren() - 5))
        end
    end
    
    return table.concat(info, "\n")
end

local function scanArea()
    local result = {"=== SCAN RESULTS ==="}
    local count = 0
    
    for _, obj in pairs(workspace:GetDescendants()) do
        local rootPart = obj:IsA("BasePart") and obj or obj:FindFirstChildOfClass("BasePart") or obj.PrimaryPart
        if rootPart and (rootPart.Position - getPlayerPosition()).Magnitude <= radius then
            local success, info = pcall(getExtendedInfo, obj)
            table.insert(result, "\n"..(success and info or "ðŸš« Failed to get info for this object"))
            count = count + 1
        end
    end
    
    table.insert(result, string.format("\n=== Found %d objects in %dm radius ===", count, radius))
    
    local output = table.concat(result, "\n")
    setclipboard(output)
    print(output)
    print("\n[SUCCESS] Data copied to clipboard!")
end

-- ÐŸÐµÑ€Ð²Ñ‹Ð¹ Ð·Ð°Ð¿ÑƒÑÐº
scanArea()

-- ÐÐ²Ñ‚Ð¾Ð¼Ð°Ñ‚Ð¸Ñ‡ÐµÑÐºÐ¾Ðµ Ð¾Ð±Ð½Ð¾Ð²Ð»ÐµÐ½Ð¸Ðµ (Ð¿Ð¾ Ð¶ÐµÐ»Ð°Ð½Ð¸ÑŽ)
while true do
    wait(30)
    scanArea()
end
