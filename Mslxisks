local success, bandage = pcall(function()
    return workspace:WaitForChild("RuntimeItems"):WaitForChild("Bandage")
end)

if success and bandage then
    local attributes = ""
    for attr, value in pairs(bandage:GetAttributes()) do
        attributes = attributes .. "\n" .. attr .. " = " .. tostring(value)
    end
    print("Атрибуты бандажа:"..attributes)
    pcall(function() setclipboard(attributes) end)
else
    warn("Бандаж не найден.")
end
